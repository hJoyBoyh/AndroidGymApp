<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetExercices"
        tools:targetApi="31">
        <activity
            android:name=".UpdateActivity"
            android:label="MODIFIER UN EXERCICE"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".AddExerciceActivity"
            android:parentActivityName=".MainActivity"
            android:label="AJOUTER UN EXERCICE"
            android:exported="false" />
        <activity
            android:name=".ExerciceChoisiActivity"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".CategorieChosisActivity"

            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>